{
    "name": "OnlyOffice/DocSpaceApiSdk",
    "type": "library",
    "version": "4.0.0",
    "description": "A simple PHP SDK for integrating with the ONLYOFFICE DocSpace API",
    "keywords": [
        "onlyoffice",
        "docspace",
        "sdk"
    ],
    "homepage": "https://www.onlyoffice.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ascensio System SIA",
            "homepage": "https://www.onlyoffice.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ONLYOFFICE/docspace-api-sdk-php/issues",
        "source": "https://github.com/ONLYOFFICE/docspace-api-sdk-php",
        "email": "support@onlyoffice.com",
        "forum": "https://forum.onlyoffice.com/"
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
